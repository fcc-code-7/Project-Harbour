@model FYP.Web.ViewModels.StudentGroupViewModel
@{
    ViewData["Title"] = "Student Groups";
    int counter = 0;
}

<!-- BEGIN: Content-->
<div id="ChangeDiv">
    <div class="app-content content">
        <div class="content-overlay"></div>
        <div class="content-wrapper">
            <div class="content-header row">
            </div>
            <div class="content-body ">
                <section class="users-list-wrapper">
                    <div class="users-list-filter px-1">
                        <!-- Section 1 -->
                        <div class=" ">
                            <div class="p-3 bg-white text-black  ">
                           

                                <div class="" style="min-height:100px;" id="container">
                                    <!-- Left content (div1) -->
                                    <div class="w-full grid sm:grid-cols-1 lg:grid-cols-2 gap-4 ">
                                        <div class=" text-2xl font-bold">
                                            <h1>
                                                Student Groups
                                            </h1>
                                        </div>

                                    </div>
                                    @if (Model.StudentGroups.Count() > 2)
                                    {
                                        
                                    <button type="button" class="btn btn-sm AddButton text-white" id="CreateGroup">Create New Group</button>
                                    }
                                    @if (Model.StudentGroups.Any() && Model.StudentGroups!=null)
                                    {
                                        <!-- Batch -->
                                        <div class="col-12 col-lg-12 col-md-12 col-sm-12 order-sm-1 order-md-1 order-lg-1 mt-2">
                                            <div class="form-group">
                                                <label>Batch</label>
                                                <fieldset>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text" id="basic-addon3">
                                                                <i class="fa fa-user-circle-o" style="color: transparent; -webkit-text-stroke: 1px black;"></i>
                                                            </span>
                                                        </div>

                                                        @if (Model.Batches != null)
                                                        {
                                                            <select class="form-control" id="BatchSelect">
                                                                <option value="" selected>Select Batch</option>
                                                                @foreach (var item in Model.Batches)
                                                                {
                                                                    if (Model.Batch != null && Model.Batch == item)
                                                                    {

                                                                        <option value="@item" selected>@item</option>
                                                                    }
                                                                    else
                                                                    {
                                                                        <option value="@item">@item</option>
                                                                    }
                                                                }
                                                            </select>
                                                        }

                                                    </div>
                                                    <span asp-validation-for="Batches" class="text-danger"></span>
                                                </fieldset>
                                            </div>
                                        </div>
                                        <!-- Group -->
                                        <div class="col-12 col-lg-12 col-md-12 col-sm-12 order-sm-2 order-md-2 order-lg-2 mb-2">
                                            <div class="form-group">
                                                <label>Group Name</label>
                                                <fieldset>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text" id="basic-addon3">
                                                                <i class="fa fa-user-circle-o" style="color: transparent; -webkit-text-stroke: 1px black;"></i>
                                                            </span>
                                                        </div>

                                                        <input type="text" class="form-control" placeholder="Enter Group Name to Search" id="GroupNameCheck" aria-describedby="basic-addon1" required>

                                                    </div>
                                                </fieldset>
                                            </div>
                                        </div>
                                      
                                    }
                                </div>

                                @if (Model.StudentGroups == null || !Model.StudentGroups.Any())
                                {
                                    <div class="container">
                                        <div class="row justify-content-center align-items-center" style="min-height: 40vh;">
                                            <div class="col-12 col-md-6 col-lg-4 d-flex flex-column justify-content-center align-items-center">
                                                <!-- Trash Icon -->
                                                <i class="fa fa-trash-o text-secondary" style="font-size: 10rem; display: block;"></i>
                                                <!-- Text Below the Icon -->
                                                <p class="text-secondary fw-bold text-center colorChange" style="font-size: 1.5rem;">No Group Exist!</p>
                                            </div>
                                        </div>
                                    </div>
                                }
                                else
                                {
                                  
                                   <div>
                                       <label>Groups:</label>
                                       </div>
                                    <div class="container mt-0 md:mt-5 lg:mt-5">
                                        <div class="row d-flex justify-content-center " id="GroupContainer">

                                            @foreach (var item in Model.StudentGroups)
                                            {
                                                <div class="col-12 col-md-12 col-sm-12 col-lg-6">
                                                    <div class="card p-0">
                                                        <div class="card-header no-border d-flex align-items-center">
                                                            <div class="widget-49-date-warning d-flex align-items-center">
                                                                <span class="badge bg-warning rounded-circle text-dark d-inline-flex align-items-center justify-content-center" style="width: 24px; height: 24px;">
                                                                    @(counter = counter + 1)
                                                                </span>
@*                                                                 <span class="mx-2">@item.Name</span>
 *@                                                            </div>
                                                            <div class="ml-auto " >
                                                                <!-- Edit Icon -->
                                                                <a href="javascript:void(0)" class="EditButton editBTN " style="margin-left:1rem;" data-id="@item.Id" title="Edit">
                                                                    <i class="fas fa-edit"></i>
                                                                </a>
                                                                <!-- Details Icon -->
                                                                <a href="javascript:void(0)" class="DetailsButton editBTN" style="margin-left:1rem;" data-id="@item.Id" title="Details">
                                                                    <i class="fas fa-info-circle"></i>
                                                                </a>
                                                                <!-- Assign Icon -->
                                                                <a href="javascript:void(0)" class="AssignButton1 assignBTN" style="margin-left:1rem;" data-id="@item.Id" title="Assign">
                                                                    <i class="fas fa-user-plus"></i>
                                                                </a>
                                                                <!-- Delete Icon -->
                                                                <a href="javascript:void(0)" class="DeleteButton deleteBTN " style="margin-left:1rem;" data-id="@item.Id" title="Delete">
                                                                    <i class="fas fa-trash-alt"></i>
                                                                </a>
                                                            </div>

                                                        </div>
                                                        <div class="card-body pt-0">
                                                            <div class="widget-49">
                                                                <div class="table-responsive" style="overflow-x: auto;">
                                                                    <table class="  mb-0" style="border-spacing: 0; min-width: 200px;">
                                                                        <tr>
                                                                            <td class="">
                                                                                <span class="text-black fw-bold">Group:
                                                                                <span class="text-green-600 text-sm font-medium dark:bg-green-900 dark:text-green-300" style="margin-left:2.4rem">@item.Name</span>
                                                                            </td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td class="">
                                                                                <span class="text-black fw-bold">Member1:</span>
                                                                                <span class="text-green-600 text-sm font-medium dark:bg-green-900 dark:text-green-300 mx-1">@item.LeaderName</span>
                                                                            </td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td class="">
                                                                                <span class="text-black fw-bold">Member2:</span>
                                                                                <span class="text-green-600 text-sm font-medium dark:bg-green-900 dark:text-green-300 mx-1">@item.member1</span>
                                                                            </td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td class="">
                                                                                <span class="text-black fw-bold">Member3:</span>
                                                                                <span class="text-green-600 text-sm font-medium dark:bg-green-900 dark:text-green-300 mx-1">@item.Member2</span>
                                                                            </td>
                                                                        </tr>
                                                                    </table>
                                                                   
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            }

                                            @if (Model.StudentGroups.Count() < 2)
                                            {
                                                <div class="col-12 col-md-6 col-sm-12 col-lg-3 btn" id="CreateGroup">
                                                    <div class="card text-center ">
                                                        <div class="card-body ">
                                                            <i class="fa fa-plus fa-10x colorchange"></i> <!-- Large Plus Icon -->
                                                        </div>
                                                    </div>
                                                </div>
                                            }
                                        </div>
                                    </div>
                                }

                            </div>
                        </div>
                </section>
            </div>
        </div>
    </div>
    <!-- Delete Modal -->
    <div class="modal fade" id="deleteRoomModal" tabindex="-1" role="dialog" aria-labelledby="deleteModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="deleteModalLabel">Delete Room</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    Are you sure you want to delete this Room Incharge?
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-danger" id="confirmDelete">Delete</button>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- END: Content-->
<!-- Modal -->
<script>
    $(document).ready(function () {
        $("#CreateGroup").click(function () {
            $.ajax({
                url: '/supervisor/create',
                type: "GET",
                success: function (response) {
                    $("#MainContainer").html(response);
                },
                error: function () {

                }
            });
        });

       @*  $('.changeSupervisor').on('click', function () {
            var groupId = $(this).data("value");
            $.ajax({
                url: '/supervisor/ChangeSupervisor',
                type: "GET",
                data: { groupId: groupId },
                success: function (response) {
                    $("#MainContainer").html(response);
                },
                error: function () {

                }
            });
        }); *@
    @* Edit Group *@
            $(document).off('click', '#editBTN').on('click', '#editBTN', function (e) {
                var ID = $(this).data("id");
                $.ajax({
                    url: '/studentgroups/EditGroup',
                    type: "GET",
                    data: { id: ID },
                    success: function (response) {
                        $("#MainContainer").html(response);
                    },
                    error: function () {

                    }
                });
            });
        var Id = "";
        $(document).on('click', '.deleteBTN', function (e) {
            e.preventDefault();
            Id = $(this).data("id");
        });
        $(document).off('click', '#confirmDelete').on('click', '#confirmDelete', function (e) {
            e.preventDefault();
            $('#modalRoom').modal('hide');
            $('.modal-backdrop').remove();
            showLoadingSpinner();
            $.ajax({
                url: "/studentgroups/DeleteGroup",
                type: "POST",
                data: { id: Id},
                success: function (response) {
                    hideLoadingSpinner();

                    $("#MainContainer").html(response);

                },
                error: function (jqXHR, textStatus, errorThrown) {
                    hideLoadingSpinner(); // Hide the loading spinner
                    console.error("Error fetching rooms:", textStatus, errorThrown);
                    alert("An error occurred while fetching the rooms. Please try again.");
                }
            });
        });

    @* ChangeSupervisor *@
            $('.changeSupervisor').on('click', function () {
                var groupId = $(this).data("id");
                $.ajax({
                    url: '/studentgroups/ChangeSupervisor',
                    type: "GET",
                    data: { groupId: groupId },
                    success: function (response) {
                        $("#MainContainer").html(response);
                    },
                    error: function () {

                    }
                });
            });
     
        $(document).on('change', '#GroupNameCheck, #BatchSelect', function (e) {
            e.preventDefault();

            showLoadingSpinner();
            var groupWord = $("#GroupNameCheck").val();  // Get the group name from the input
            var batch = $("#BatchSelect").val();  // Get the selected batch

            // Send both GroupWord and Batch to the server
            $.ajax({
                url: "/studentgroups/FilterGroups", // URL for your controller action
                type: "POST",
                data: {
                    GroupWord: groupWord,
                    Batch: batch
                },
                success: function (response) {
                    hideLoadingSpinner();
                    console.log(response); // Log to check if the response contains correct data

                    // Clear the current group container
                    $("#GroupContainer").html('');

                    var counter = 0;
                    if (response.length > 0) {
                        $.each(response, function (index, item) {
                            console.log(item.LeaderName, item.member1, item.Member2, item.supervisorname);

                            // Create the HTML structure dynamically for each group
                            var groupHtml = `
                        <div class="col-12 col-md-12 col-sm-12 col-lg-6">
                            <div class="card p-0">
                                <div class="card-header no-border d-flex align-items-center">
                                    <div class="widget-49-date-warning d-flex align-items-center">
                                        <span class="badge bg-warning rounded-circle text-dark d-inline-flex align-items-center justify-content-center" style="width: 24px; height: 24px;">
                                            ${(counter += 1)}
                                        </span>
                                    </div>
                                    <div class="ml-auto">
                                        <!-- Edit Icon -->
                                        <a href="javascript:void(0)" class="EditButton editBTN" style="margin-left:1rem;" data-id="${item.groupId}" title="Edit">
                                            <i class="fas fa-edit"></i>
                                        </a>
                                        <!-- Details Icon -->
                                        <a href="javascript:void(0)" class="DetailsButton editBTN" style="margin-left:1rem;" data-id="${item.groupId}" title="Details">
                                            <i class="fas fa-info-circle"></i>
                                        </a>
                                        <!-- Assign Icon -->
                                        <a href="javascript:void(0)" class="AssignButton1 editBTN" style="margin-left:1rem;" data-id="${item.groupId}" title="Assign">
                                            <i class="fas fa-user-plus"></i>
                                        </a>
                                        <!-- Delete Icon -->
                                        <a href="javascript:void(0)" class="DeleteButton deleteBTN" style="margin-left:1rem;" data-id="${item.groupId}" title="Delete">
                                            <i class="fas fa-trash-alt"></i>
                                        </a>
                                    </div>
                                </div>
                                <div class="card-body pt-0">
                                    <div class="widget-49">
                                        <div class="table-responsive" style="overflow-x: auto;">
                                            <table class="mb-0" style="border-spacing: 0; min-width: 200px;">
                                                <tr>
                                                    <td>
                                                        <span class="text-black fw-bold">Group:</span>
                                                        <span class="text-green-600 text-sm font-medium dark:bg-green-900 dark:text-green-300" style="margin-left:2.4rem">${item.name}</span>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <span class="text-black fw-bold">Member1:</span>
                                                        <span class="text-green-600 text-sm font-medium dark:bg-green-900 dark:text-green-300 mx-1">${item.leaderName}</span>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <span class="text-black fw-bold">Member2:</span>
                                                        <span class="text-green-600 text-sm font-medium dark:bg-green-900 dark:text-green-300 mx-1">${item.member1}</span>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>
                                                        <span class="text-black fw-bold">Member3:</span>
                                                        <span class="text-green-600 text-sm font-medium dark:bg-green-900 dark:text-green-300 mx-1">${item.member2}</span>
                                                    </td>
                                                </tr>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        `;
                            // Append the new group card to the container
                            $("#GroupContainer").append(groupHtml);
                        });
                    } else {
                        // If no groups are found, display the "No Group Exist" message
                        var noGroupHtml = `
                    <div class="container">
                        <div class="row justify-content-center align-items-center" style="min-height: 40vh;">
                            <div class="col-12 col-md-6 col-lg-4 d-flex flex-column justify-content-center align-items-center">
                                <i class="fa fa-trash-o text-secondary" style="font-size: 10rem; display: block;"></i>
                                <p class="text-secondary fw-bold text-center colorChange" style="font-size: 1.5rem;">No Group Found!</p>
                            </div>
                        </div>
                    </div>
                    `;
                        $("#GroupContainer").append(noGroupHtml);
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    hideLoadingSpinner();
                    console.error("Error fetching groups:", textStatus, errorThrown);
                    alert("An error occurred while fetching the groups. Please try again.");
                }
            });
        });



    });
</script>

