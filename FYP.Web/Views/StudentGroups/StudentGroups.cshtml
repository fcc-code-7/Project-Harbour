@model FYP.Web.ViewModels.StudentGroupViewModel
@{
    ViewData["Title"] = "Student Groups";
    int counter = 0;
}

<!-- BEGIN: Content-->
<div id="ChangeDiv">
    <div class="app-content content">
        <div class="content-overlay"></div>
        <div class="content-wrapper">
            <div class="content-header row">
            </div>
            <div class="content-body ">
                <section class="users-list-wrapper">
                    <div class="users-list-filter px-1">
                        <!-- Section 1 -->
                        <div class="">
                            <div class="p-3 bg-white text-black rounded-lg " style="height:80vh ; min-height:80vh">

                                <!-- Column 1: -->
                                <div class="" style="min-height:100px;">
                                    <div class="w-full grid sm:grid-cols-1 lg:grid-cols-2 gap-4 ">
                                        <div class=" text-2xl font-bold">
                                            <h1>
                                                Student Groups
                                            </h1>
                                        </div>

                                    </div>
                                    @if (User.IsInRole("Supervisor"))
                                    {
                                    @if (Model.StudentGroups.Count() < 2)
                                    {

                                        <button type="button" class="btn btn-sm AddButton text-white" id="CreateGroup">Create New Group</button>
                                    }
                                    }
                                    @if (Model.StudentGroups.Any() && Model.StudentGroups != null)
                                    {
                                        <!-- Batch -->
                                        <div class="col-12 col-lg-12 col-md-12 col-sm-12 order-sm-1 order-md-1 order-lg-1 mt-1">
                                            <div class="form-group">
                                                <label>Batch</label>
                                                <fieldset>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text" id="basic-addon3">
                                                                <i class="fa fa-calendar" style="color: transparent; -webkit-text-stroke: 1px black;"></i>
                                                            </span>

                                                        </div>

                                                        @if (Model.Batches != null)
                                                        {
                                                            <select class="form-control" id="BatchNameSelect">
                                                                <option value="" selected>Select Batch</option>
                                                                @foreach (var item in Model.Batches)
                                                                {
                                                                    if (Model.Batch != null && Model.Batch == item)
                                                                    {

                                                                        <option value="@item" selected>@item</option>
                                                                    }
                                                                    else
                                                                    {
                                                                        <option value="@item">@item</option>
                                                                    }
                                                                }
                                                            </select>
                                                        }

                                                    </div>
                                                    <span asp-validation-for="Batches" class="text-danger"></span>
                                                </fieldset>
                                            </div>
                                        </div>
                                        <!-- Group -->
                                        <div class="col-12 col-lg-12 col-md-12 col-sm-12 order-sm-2 order-md-2 order-lg-2 mb-2">
                                            <div class="form-group">
                                                <label>Enrollment No./ Group Name</label>
                                                <fieldset>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text" id="basic-addon3">
                                                                <i class="fa fa-users" style="color: transparent; -webkit-text-stroke: 1px black;"></i>
                                                            </span>

                                                        </div>

                                                        <input type="text" class="form-control" placeholder="Enter Enrollment No./ Group Name to Search" id="GroupNameCheck" aria-describedby="basic-addon1" required>

                                                    </div>
                                                </fieldset>
                                            </div>
                                        </div>

                                    }
                                </div>

                                @if (Model.StudentGroups == null || !Model.StudentGroups.Any())
                                {
                                    <div class="container">
                                        <div class="row justify-content-center align-items-center" style="min-height: 40vh;">
                                            <div class="col-12 col-md-6 col-lg-4 d-flex flex-column justify-content-center align-items-center">
                                                <!-- Trash Icon -->
                                                <i class="fa fa-trash-o text-secondary" style="font-size: 10rem; display: block;"></i>
                                                <!-- Text Below the Icon -->
                                                <p class="text-secondary fw-bold text-center colorChange" style="font-size: 1.5rem;">No Group Exist!</p>
                                            </div>
                                        </div>
                                    </div>
                                }
                                else
                                {

                                    <div>
                                        <label>Groups:</label>
                                    </div>
                                    <div class="container mt-0 md:mt-5 lg:mt-5">
                                        <div class="row d-flex justify-content-center " id="GroupContainer">

                                            @foreach (var item in Model.StudentGroups)
                                            {
                                                <div class="col-12 col-md-12 col-sm-12 col-lg-6">
                                                    <div class="card p-0">
                                                        <div class="card-header no-border d-flex align-items-center">
                                                            <div class="widget-49-date-warning d-flex align-items-center">
                                                                <span class="badge bg-warning rounded-circle text-dark d-inline-flex align-items-center justify-content-center" style="width: 24px; height: 24px;">
                                                                    @(counter = counter + 1)
                                                                </span>
                                                                @*                                                                 <span class="mx-2">@item.Name</span>
                                                        *@
                                                            </div>
                                                            @if (User.IsInRole("Cordinator"))
                                                            {
                                                                <h5 class=" fs-5 fs-md-4 fs-lg-3 mx-1">@item.Name</h5>

                                                            }
                                                            @if (User.IsInRole("Supervisor"))
                                                            {
                                                                <div class="ml-auto ">
                                                                    <!-- Edit Icon -->
                                                                    <a href="javascript:void(0)" class="EditButton editBTN " style="margin-left:1rem;" data-id="@item.Id" title="Edit">
                                                                        <i class="fas fa-edit"></i>
                                                                    </a>
                                                                    @*  <!-- Details Icon -->
                                                        <a href="javascript:void(0)" class="DetailsButton editBTN" style="margin-left:1rem;" data-id="@item.Id" title="Details">
                                                        <i class="fas fa-info-circle"></i>
                                                        </a> *@
                                                                    <!-- Assign Icon -->
                                                                    <a href="javascript:void(0)" class=" changeSupervisor" style="margin-left:1rem;" data-id="@item.Id" title="Change Supervisor">
                                                                        <i class="fas fa-user-plus"></i>
                                                                    </a>
                                                                    <!-- Delete Icon -->
                                                                    <a href="javascript:void(0)" class="DeleteButton deleteBTN " style="margin-left:1rem;" data-id="@item.Id" title="Delete">
                                                                        <i class="fas fa-trash-alt"></i>
                                                                    </a>
                                                                </div>

                                                            }

                                                        </div>
                                                        <div class="card-body pt-0">
                                                            <div class="widget-49">
                                                                <div class="table-responsive" style="overflow-x: auto;">
                                                                    <table class="  mb-0" style="border-spacing: 0; min-width: 200px;">
                                                                        <tr>
                                                                            @if (User.IsInRole("Cordinator"))
                                                                            {
                                                                                <td class="">
                                                                                    <span class="text-black fw-bold">Supervisor Name:</span>
                                                                                    <u>
                                                                                        <span class="inline-flex items-center gap-x-1.5 py-1.5 px-2 rounded-full text-xs font-medium bg-yellow-100 text-yellow-800 dark:bg-yellow-800/30 dark:text-yellow-500"><b>@item.supervisorname</b></span>
                                                                                    </u>
                                                                                </td>
                                                                            }
                                                                            @if (User.IsInRole("Supervisor"))
                                                                            {
                                                                                <td class="">
                                                                                    <span class="text-black fw-bold">Group Name:</span>
                                                                                    <u> <span class="badge badge-success text-dark" style="">@item.Name</span></u>
                                                                                </td>
                                                                            }
                                                                           
                                                                        </tr>
                                                                        <tr>
                                                                            <td class="">
                                                                                <span class="text-black fw-bold">Member1:</span>
                                                                                <u><span class="text-green-600 text-sm font-medium dark:bg-green-900 dark:text-green-300 mx-1"><b>@item.LeaderName</b></span></u>
                                                                            </td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td class="">
                                                                                <span class="text-black fw-bold">Member2:</span>
                                                                               <u> <span class="text-green-600 text-sm font-medium dark:bg-green-900 dark:text-green-300 mx-1"><b>@item.member1</b></span></u>
                                                                            </td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td class="">
                                                                                <span class="text-black fw-bold">Member3:</span>
                                                                                <u> <span class="text-green-600 text-sm font-medium dark:bg-green-900 dark:text-green-300 mx-1"><b>@item.Member2</b></span></u>
                                                                            </td>
                                                                        </tr>
                                                                    </table>

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            }


                                        </div>
                                    </div>
                                }

                            </div>
                        </div>
                </section>
            </div>
        </div>
    </div>
    <!-- Delete Modal -->
    <div class="modal fade" id="modalGrouo" tabindex="-1" role="dialog" aria-labelledby="modalGrouoLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="modalGrouoLabel">Delete Group</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    Are you sure you want to delete this Group?
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-danger" id="confirmDelete">Delete</button>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- END: Content-->
