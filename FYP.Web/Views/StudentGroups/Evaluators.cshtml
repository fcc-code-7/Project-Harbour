<script>
    $(document).ready(function () {
        $('#modalRoom').on('hidden.bs.modal', function () {
            $('#NewRoom').val(''); // Reset the input field when modal is closed
        });
       

       
        $(document).on('click', '#AddRoom', function (e) {
            e.preventDefault();
            $('#modalRoom').modal('show');
        });
      
        $(document).on('click', '#modalCloseRoom', function (e) {
            e.preventDefault();
            $('#modalRoom').modal('hide');
        });
        $(document).off('click', '#SaveRoom').on('click', '#SaveRoom', function (e) {
            showLoadingSpinner(); // Show the loading spinner
            var roomNo = $("#NewRoom").val(); // Use the new ID for the modal input field
            $.ajax({
                url: "/studentgroups/FetchRoom",
                type: "POST",
                data: { room: roomNo},
                success: function (response) {
                    $('#modalRoom').modal('hide'); // Hide the modal
                    hideLoadingSpinner(); // Hide the loading spinner

                    // Clear the current RoomContainer
                    $("#RoomContainer").empty();

                    // Build new select element
                    var selectHtml = '<div class="form-group"><div style="display: flex; justify-content: space-between; align-items: center;">'
                        + '<label>Room <span class="text-danger">(if not Listed click Add)</span></label>'
                        + '<a class="custom-badge-green" id="AddRoom">Add</a></div>'
                        + '<fieldset><div class="input-group">'
                        + '<div class="input-group-prepend">'
                        + '<span class="input-group-text" id="basic-addon3">'
                        + '<i class="fa fa-home" style="color: transparent; -webkit-text-stroke: 1px black;"></i></span>'
                        + '</div>'
                        + '<select id="Room" class="form-control">';

                    // Default option
                    selectHtml += '<option value="">Select Room</option>';

                    // Check if response.room.rooms exists and is an array
                    if (response.room && Array.isArray(response.room.rooms) && response.room.rooms.length > 0) {
                        response.room.rooms.forEach(function (item) {
                            selectHtml += '<option value="' + item.id + '">' + item.roomNo + '</option>';
                        });
                    } else {
                        selectHtml += '<option value="">No rooms available. Add one first!</option>';
                    }

                    selectHtml += '</select></div>'
                        + '<span asp-validation-for="Room" class="text-danger"></span></fieldset></div>';

                    // Append the new select element to RoomContainer
                    $("#RoomContainer").append(selectHtml);
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    hideLoadingSpinner(); // Hide the loading spinner
                    console.error("Error fetching rooms:", textStatus, errorThrown);
                    alert("An error occurred while fetching the rooms. Please try again.");
                }
            });
        });
        $(document).off('change', '.groupID').on('change', '.groupID', function (e) {
            e.preventDefault();
            showLoadingSpinner(); // Show the loading spinner
            var groupId = $(this).val();
            var batches = $("#batches").val();
            var evaluation = $("#Evaluation").val();
            $.ajax({
                url: "/studentgroups/FetchBatches",
                type: "GET",
                data: { id: groupId, batch: batches, EvalType: evaluation },
                success: function (response) {
                    hideLoadingSpinner(); // Hide the loading spinner
                    $("#NewContainer").html(response);

                },
                error: function () {

                }
            });

        });
        //yahan changekrna hai
        $(document).off('change', '#Member1ID').on('change', '#Member1ID', function (e) {
            e.preventDefault();
            showLoadingSpinner(); // Show the loading spinner
            var groupId = $("#groupIDs").val();
            var batches = $("#batches").val();
            var evaluation = $("#Evaluation").val();
            var Room = $("#Room").val();
            var AppointedDate = $("#AppointedDate").val();
            var AppointedTime = $("#AppointedTime").val();
            var member1 = $(this).val();
            $.ajax({
                url: "/studentgroups/FetchBatches",
                type: "GET",
                data: { id: groupId, batch: batches, EvalType: evaluation, Time: AppointedTime, Date: AppointedDate, sup1: member1 , room : Room },
                success: function (response) {
                    hideLoadingSpinner(); // Hide the loading spinner
                    $("#NewContainer").html(response);

                },
                error: function () {

                }
            });

        });

        $(document).off('change', '.Room').on('change', '.Room', function (e) {
            e.preventDefault();
            showLoadingSpinner(); // Show the loading spinner
            var roomNo = $(this).val();
            var groupId = $("#groupIDs").val();
            var batches = $("#batches").val();
            var evaluation = $("#Evaluation").val();
            var AppointedDate = $("#AppointedDate").val();
            var AppointedTime = $("#AppointedTime").val();

            $.ajax({
                url: "/studentgroups/FetchBatches",
                type: "GET",
                data: { id: groupId, batch: batches, EvalType: evaluation, room: roomNo, Time: AppointedTime, Date: AppointedDate },
                success: function (response) {
                    hideLoadingSpinner(); // Hide the loading spinner
                    $("#NewContainer").html(response);

                },
                error: function () {

                }
            });

        });
        $(document).off('change', '.batchSelect').on('change', '.batchSelect', function (e) {
            var getbatch = $(this).val();
            showLoadingSpinner(); // Show the loading spinner
            $.ajax({
                url: "/studentgroups/FetchBatches",
                type: "GET",
