@model FYP.Web.ViewModels.FYPCommitteViewModel


@{

}
<!-- BEGIN: Content-->
<div id="NewContainer">
    <div class="app-content content">
        <div class="content-overlay"></div>
        <div class="content-wrapper">
            <div class="content-header row">
            </div>
            <div class="content-body ">
                <section class="users-list-wrapper">
                    <div class="users-list-filter px-1">
                        <!-- Section 1 -->
                        <div class=" ">
                            <div class="p-3 bg-white text-black ">

                                <!-- Column 1: -->
                                <div class="" style="min-height:100px">
                                    <div class="w-full grid sm:grid-cols-1 lg:grid-cols-2 gap-4 ">
                                        <div class=" text-2xl font-bold">
                                            <h1>
                                                Evaluators
                                            </h1>
                                        </div>

                                    </div>
                                    <form class="form scrollable-div overflow-auto p-1" id="submitform">
                                        <div class="modal-body">
                                            <div class="form-body">
                                                <div class="row d-flex justify-content-center">
                                                    <div class="col-12 col-lg-12 col-md-12 col-sm-12 ">
                                                        <input type="hidden" value="@Model.groupID" name="groupIDs" id="groupIDs" />
                                                        <input type="hidden" value="@Model.batch" name="batches" id="batches" />
                                                        <input type="hidden" value="@Model.EvaluationID" name="Evaluation" id="Evaluation" />
                                                        @* Batch *@
                                                        <div class="form-group">
                                                            <label>Batch</label>

                                                            <fieldset>
                                                                <div class="input-group">
                                                                    <div class="input-group-prepend">
                                                                        <span class="input-group-text " id="basic-addon3">
                                                                            <i class="fa fa-graduation-cap" style="color: transparent; -webkit-text-stroke: 1px black;"></i>
                                                                        </span>
                                                                    </div>
                                                                    @if (Model.batches != null)
                                                                    {
                                                                        <select asp-for="batch" name="batch" id="batch" class="form-control batchSelect">
                                                                            <option value="">Select Batch</option>

                                                                            @foreach (var item in Model.batches)
                                                                            {
                                                                                if (Model.batch != null && item == Model.batch)
                                                                                {
                                                                                    <option value="@item" selected>@item</option>
                                                                                }
                                                                                else
                                                                                {
                                                                                    <option value="@item">@item</option>
                                                                                }
                                                                            }
                                                                        </select>
                                                                    }
                                                                    else
                                                                    {
                                                                        <input type="text" class="form-control" value="No Batch Exist" aria-describedby="basic-addon1" readonly required>
                                                                    }

                                                                </div>
                                                                <span asp-validation-for="batch" class="text-danger"></span>
                                                            </fieldset>
                                                        </div>
                                                    </div>
                                                    @* Evaluation *@
                                                    <div class="col-12 col-lg-12 col-md-12 col-sm-12 ">
                                                        @* Evaluation *@
                                                        <div class="form-group">
                                                            <label>Evaluation</label>

                                                            <fieldset>
                                                                <div class="input-group">
                                                                    <div class="input-group-prepend">
                                                                        <span class="input-group-text " id="basic-addon3">
                                                                            <i class="fa fa-pencil" style="color: transparent; -webkit-text-stroke: 1px black;"></i>
                                                                        </span>
                                                                    </div>
                                                                    @if (Model.batch != null)
                                                                    {
                                                                        @Html.DropDownListFor(model => model.EvaluationID,
                                                                                 new SelectList(new List<string> { "Proposal", "Mid", "Final" }, Model.EvaluationID),
                                                                                 "Select Evaluation",
                                                                                 new { @class = "form-control EvaluationSelect", id = "EvaluationID" })
                                                                        <span asp-validation-for="EvaluationID" class="text-danger"></span>
                                                                    }
                                                                    else
                                                                    {
                                                                        <input type="text" class="form-control" value="Select Batch First" readonly required>
                                                                    }

                                                                </div>
                                                                <span asp-validation-for="EvaluationID" class="text-danger"></span>
                                                            </fieldset>
                                                        </div>
                                                    </div>

                                                    @if (Model.EvaluationID == null || Model.CheckDates == null)
                                                    {
                                                        if (Model.EvaluationID != null && Model.CheckDates == null)
                                                        {
                                                            <div class="container">
                                                                <div class="row justify-content-center align-items-center" style="min-height: 40vh;">
                                                                    <div class="col-12 col-md-6 col-lg-4 d-flex flex-column justify-content-center align-items-center">
                                                                        <i class="fa fa-exclamation-triangle" style="font-size: 10rem; color:#dc2626;"></i>
                                                                            <p class="text-secondary fw-bold text-center text-danger" style="font-size: 1.5rem;">First Assign Evaluation Date in EvalType!</p>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        }else{

                                                        <div class="container">
                                                            <div class="row justify-content-center align-items-center" style="min-height: 40vh;">
                                                                <div class="col-12 col-md-6 col-lg-4 d-flex flex-column justify-content-center align-items-center">
                                                                        <i class="fa fa-exclamation-triangle" style="font-size: 10rem; color:#dc2626;"></i>
                                                                    <p class="text-secondary fw-bold text-center text-danger" style="font-size: 1.5rem;">Select Evaluation</p>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        }
                                                    }
                                                    else{
                                                    <div class="col-12 col-lg-12 col-md-12 col-sm-12 ">
                                                        @* Group *@
                                                        <div class="form-group">
                                                            <label>Groups</label>

                                                            <fieldset>
                                                                <div class="input-group">
                                                                    <div class="input-group-prepend">
                                                                        <span class="input-group-text " id="basic-addon3">
                                                                            <i class="fa fa-users" style="color: transparent; -webkit-text-stroke: 1px black;"></i>
                                                                        </span>
                                                                    </div>
                                                                    @if (Model.studentGroups != null && Model.EvaluationID != null)
                                                                    {
                                                                        <select asp-for="groupID" name="groupID" id="groupID" class="form-control groupID">
                                                                            <option value="">Select Student Groups</option>

                                                                            @foreach (var item in Model.studentGroups)
                                                                            {
                                                                                if (Model.groupID != null && item.groupID == Model.groupID)
                                                                                {
                                                                                    <option value="@item.groupID" selected>@item.groupName</option>
                                                                                }
                                                                                else
                                                                                {
                                                                                    <option value="@item.groupID">@item.groupName</option>
                                                                                }
                                                                            }
                                                                        </select>
                                                                    }
                                                                    else
                                                                    {
                                                                        <input type="text" class="form-control" value="Select Evaluation First" aria-describedby="basic-addon1" readonly required>
                                                                    }

                                                                </div>
                                                                <span asp-validation-for="groupID" class="text-danger"></span>
                                                            </fieldset>
                                                        </div>
                                                    </div>
                                                    @* Room *@
                                                    <div class="col-12 col-lg-12 col-md-12 col-sm-12 ">
                                                        <div id="RoomContainer">
                                                            <div class="form-group">
                                                                <div style="display: flex; justify-content: space-between; align-items: center;">
                                                                    <label>Room <span class="text-danger">(if not Listed click Add)</span></label>
                                                                    <a class="custom-badge-green" id="AddRoom">Add</a>
                                                                </div>
                                                                <fieldset>
                                                                    <div class="input-group">
                                                                        <div class="input-group-prepend">
                                                                            <span class="input-group-text" id="basic-addon3">
                                                                                <i class="fa fa-home" style="color: transparent; -webkit-text-stroke: 1px black;"></i>
                                                                            </span>
                                                                        </div>
                                                                        @if (Model.EvaluationID != null)
                                                                        {
                                                                            @if (Model.Rooms != null)
                                                                            {
                                                                                <select asp-for="Room" name="Room" id="Room" class="form-control Room">
                                                                                    <option value="">Select Room</option>
                                                                                    @foreach (var item in Model.Rooms)
                                                                                    {
                                                                                        if (Model.Rooms != null && item.Id == Model.Room)
                                                                                        {
                                                                                            <option value="@item.Id" selected>@item.RoomNo</option>
                                                                                        }
                                                                                        else
                                                                                        {
                                                                                            <option value="@item.Id">@item.RoomNo</option>
                                                                                        }
                                                                                    }
                                                                                </select>
                                                                            }
                                                                            else
                                                                            {
                                                                                <select class="form-control">
                                                                                    <option value="" selected>Add Room First</option>
                                                                                </select>
                                                                            }
                                                                        }
                                                                        else
                                                                        {
                                                                            <input type="text" class="form-control" value="Select Group First" aria-describedby="basic-addon1" readonly required>
                                                                        }
                                                                    </div>
                                                                    <span asp-validation-for="Rooms" class="text-danger"></span>
                                                                </fieldset>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    @* Date *@
                                                    <div class="col-12 col-lg-12 col-md-12 col-sm-12 ">
                                                        <div class="form-group">
                                                            <label>Date</label>

                                                            <fieldset>
                                                                <div class="input-group">

                                                                    <div class="input-group-prepend">
                                                                        <span class="input-group-text " id="basic-addon1"><i class="feather icon-calendar"></i></span>
                                                                    </div>
                                                                    <input type="date" class="form-control" id="AppointedDate" placeholder="Enter AppointedDate" asp-for="AppointedDate" aria-describedby="basic-addon1" name="AppointedDate" required>
                                                                </div>
                                                                <span asp-validation-for="AppointedDate" class="text-danger"></span>
                                                            </fieldset>
                                                        </div>
                                                    </div>
                                                    @* Time *@
                                                    <div class="col-12 col-lg-12 col-md-12 col-sm-12 ">
                                                        <div class="form-group">
                                                            <label>Time  <span class="text-danger">(Automatically Adjusted according to Room No)</span></label>

                                                            <fieldset>
                                                                <div class="input-group">

                                                                    <div class="input-group-prepend">
                                                                        <span class="input-group-text " id="basic-addon1"><i class="feather icon-clock"></i></span>
                                                                    </div>
                                                                    <input type="time" class="form-control" id="AppointedTime" placeholder="Enter Time" asp-for="AppointedTime" aria-describedby="basic-addon1" name="AppointedTime" required>



                                                                </div>
                                                                <span asp-validation-for="AppointedTime" class="text-danger"></span>
                                                            </fieldset>
                                                        </div>
                                                    </div>
                                                  
                                                    @* Evaluator1 *@
                                                    <div class="col-12 col-lg-12 col-md-12 col-sm-12 ">
                                                        <div class="form-group">
                                                            <label>Evaluator 1</label>

                                                            <fieldset>
                                                                <div class="input-group">
                                                                    <div class="input-group-prepend">
                                                                        <span class="input-group-text " id="basic-addon3"><i class="fa fa-user-circle-o" style="color: transparent; -webkit-text-stroke: 1px black;"></i></span>
                                                                    </div>
                                                                    @if (Model.SupervisorList1 != null)
                                                                    {
                                                                        <select asp-for="Member1ID" name="Member1ID" id="Member1ID" class="form-control">
                                                                            <option value="">Select Evaluator</option>

                                                                            @foreach (var item in Model.SupervisorList1)
                                                                            {
                                                                                if (Model.Member1ID != null && item.SupervisorID == Model.Member1ID)
                                                                                {
                                                                                    <option value="@item.SupervisorID" selected>@item.SupervisorName</option>
                                                                                }
                                                                                else
                                                                                {
                                                                                    <option value="@item.SupervisorID">@item.SupervisorName</option>
                                                                                }
                                                                            }

                                                                        </select>
                                                                    }
                                                                    else
                                                                    {
                                                                        <input type="text" class="form-control" value="Select Batch First" aria-describedby="basic-addon1" readonly required>
                                                                    }
                                                                </div>
                                                                <span asp-validation-for="Member1ID" class="text-danger"></span>
                                                            </fieldset>
                                                        </div>
                                                    </div>
                                                    @* Evaluator2 *@
                                                    <div class="col-12 col-lg-12 col-md-12 col-sm-12 ">

                                                        <div class="form-group">
                                                            <label>Evaluator 2</label>

                                                            <fieldset>
                                                                <div class="input-group">
                                                                    <div class="input-group-prepend">
                                                                        <span class="input-group-text " id="basic-addon3"><i class="fa fa-user-circle-o" style="color: transparent; -webkit-text-stroke: 1px black;"></i></span>
                                                                    </div>
                                                                    @if (Model.SupervisorList2 != null)
                                                                    {
                                                                        <select asp-for="Member2ID" name="Member2ID" id="Member2ID" class="form-control">
                                                                            <option value="">Select Evaluator</option>

                                                                            @foreach (var item in Model.SupervisorList2)
                                                                            {
                                                                                if (Model.Member2ID != null && item.SupervisorID == Model.Member2ID)
                                                                                {
                                                                                    <option value="@item.SupervisorID" selected>@item.SupervisorName</option>
                                                                                }
                                                                                else
                                                                                {
                                                                                    <option value="@item.SupervisorID">@item.SupervisorName</option>
                                                                                }
                                                                            }

                                                                        </select>
                                                                    }
                                                                    else
                                                                    {
                                                                        <input type="text" class="form-control" value="Select Batch First" aria-describedby="basic-addon1" readonly required>
                                                                    }
                                                                </div>
                                                                <span asp-validation-for="Member2ID" class="text-danger"></span>
                                                            </fieldset>
                                                        </div>
                                                    </div>
                                                    @if (Model.assignRequest == "true")
                                                    {
                                                        <input type="hidden" name="assignRequest" id="AssignRequest" value="true" />

                                                    }
                                                    else
                                                    {
                                                        <input type="hidden" name="assignRequest" id="AssignRequest" value="false" />
                                                    }
                                                    }
                                                </div>
                                            
                                               
                                               
                                                
                                            </div>
                                        </div>

                                        @if (Model.CheckDates != null)
                                        {
                                <div class="modal-footer">
                                    <button type="submit" class="btn btn-outline-primary " id="EvaluatorFormSubmit">Save</button>
                                </div>
                                }
                                </form>

                            </div>
                        </div>
                    </div>

            </div>
            </section>
        </div>
    </div>
</div>
<div class="modal fade " id="modalRoom" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Add Room</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body d-flex justify-content-center">
                <div class="col-12 col-lg-12 col-md-12 col-sm-12">
                    <div class="form-group">
                        <label>Room No</label>
                        <fieldset>
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text" id="basic-addon1">
                                        <i class="fa fa-home" style="color: transparent; -webkit-text-stroke: 1px black;"></i>
                                    </span>
                                </div>
                                <!-- Changed the input field ID -->
                                <input type="text" class="form-control" id="NewRoom" placeholder="Enter Room No" aria-describedby="basic-addon1" name="Room" required>
                            </div>
                            <span asp-validation-for="Room" class="text-danger"></span>
                        </fieldset>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal" id="modalCloseRoom">Close</button>
                <button type="button" class="btn btn-primary" id="SaveRoom">Save changes</button>
            </div>
        </div>
    </div>
</div>

</div>
<!-- END: Content-->
<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>


